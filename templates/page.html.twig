{#
/**
 * Page.html.twig from stanford_basic.
 */
#}
{%- extends "@stanford_basic/page.html.twig" -%}

{# Change the brand bar. #}
{%- set brand_bar_variant = "su-brand-bar--dark" -%}

{# Masthead Section. #}
{%- block block_header -%}
  {%- if page.header or page.search or page.menu -%}
    <header class="su-masthead su-masthead--right ">
      <section class="chemh-masthead">
        {{ page.header }}
        {{ page.menu }}
        <a href="/search" id="chemh-button--search-link" class="su-site-search__submit"><span class="visually-hidden">Search</span></a>
        {{ page.search }}
      </section>
    </header>
  {%- endif -%}
{%- endblock -%}
